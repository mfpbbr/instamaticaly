<div class="row-fluid">
 <div class="span12">
  <h1><%= @user.username %>'s feed</h1>    
 </div>
</div><!--/row-->


<div class="row-fluid">
 <div class="span12">
  <% @recent_media_items.each do |media| %>
  <div style="float:left; background: url('<%= media.images.thumbnail.url %>'); background-repeat:no-repeat; height:150px; width:150px; padding-left:5px; padding-top:5px;"><span class="badge badge-info"><%= media.likes.count %></span></div>
  <% end %>    
 </div>
</div><!--/row-->


